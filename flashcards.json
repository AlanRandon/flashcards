{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"cards": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"term": {
						"type": "string"
					},
					"definition": {
						"type": "string"
					}
				},
				"required": ["term", "definition"]
			}
		},
		"topics": {
			"type": "array",
			"items": {
				"type": "string"
			}
		}
	}
}
